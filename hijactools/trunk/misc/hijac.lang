<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language SYSTEM "language.dtd">
<language _name="HiJaC Report" version="1.0" _section="Others">
  <metadata>
    <property name="mimetypes">text/plain</property>
    <property name="globs">*.hijac</property>
  </metadata>
  <syntax-item _name="F" style="colour:red">
    <start-regex>\[F\]</start-regex>
    <end-regex>\)|$</end-regex>
  </syntax-item>
  <syntax-item _name="J" style="colour:green">
    <start-regex>\[J\]</start-regex>
    <end-regex>\)|$</end-regex>
  </syntax-item>
  <syntax-item _name="scj" style="colour:red">
    <start-regex>\[SCJ\]</start-regex>
    <end-regex>\)|$</end-regex>
  </syntax-item>
  <syntax-item _name="java" style="colour:green">
    <start-regex>\[JAVA\]</start-regex>
    <end-regex>\)|$</end-regex>
  </syntax-item>
  <syntax-item _name="arrow" style="colour:red">
    <start-regex>\-></start-regex>
    <end-regex>\)|$</end-regex>
  </syntax-item>
  <keyword-list _name="keywords:red" style="colour:red">
    <keyword>WARNING</keyword>
    <keyword>ENUM</keyword>
    <keyword>INTERFACE</keyword>
    <keyword>INNER</keyword>
    <keyword>LOCAL</keyword>
    <keyword>ANONYMOUS</keyword>
    <keyword>CALLS_FRAMEWORK</keyword>
  </keyword-list>
  <keyword-list _name="keywords:blue" style="colour:blue">
    <keyword>EXTENDS</keyword>
    <keyword>IMPLEMENTS</keyword>
    <keyword>ALLOCATES</keyword>
    <keyword>MAY_SUSPEND</keyword>
  </keyword-list>
  <keyword-list _name="keywords:green" style="colour:green">
    <keyword>ABSTRACT</keyword>
  </keyword-list>
  <keyword-list _name="keywords:bold" style="style:bold">
    <keyword>CTOR</keyword>
    <keyword>STATIC</keyword>
  </keyword-list>
</language>
