\begin{circus}
\circprocess MidMissionApp \circdef  \circbegin
\end{circus}

\begin{schema}{State}
  this : \circreftype MidMissionClass\\ 
\end{schema}
% 
\begin{circusaction}
\circstate State
\end{circusaction}
%
\begin{schema}{Init}
  State~' \\
\where
  this' = \circnew MidMissionClass()\\ 
\end{schema}

\begin{circusaction}
InitializePhase \circdef \\
\circblockopen
  initializeCall~.~MidMissionMID \then \\
	register~!~BottomMissionSequencerASID~!~MidMissionMID \then \\
	register~!~BottomMissionSequencerBSID~!~MidMissionMID \then \\
  initializeRet~.~MidMissionMID \then \\
  \Skip
\circblockclose
\end{circusaction}

\begin{circusaction}
CleanupPhase \circdef
\\
\circblockopen
 cleanupMissionCall~.~MidMissionMID \then \\
 cleanupMissionRet~.~MidMissionMID~!~\true \then \\
 \Skip
\circblockclose
\end{circusaction}

%
%

\begin{circusaction}
Methods \circdef
\circblockopen
	InitializePhase \\
	\extchoice \\
	CleanupPhase \\
%
%
%
\circblockclose
\circseq Methods
\end{circusaction}

\begin{circusaction}
\circspot ( \lschexpract Init \rschexpract \circseq Methods) 
 \circinterrupt (end\_mission\_app~.~MidMissionMID \then \Skip)
\end{circusaction}

\begin{circus}
  \circend
\end{circus}
