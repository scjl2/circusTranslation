\begin{circusaction}
GetNextMission \circdef \circvar ret : MissionID \circspot \\
\circblockopen
    getNextMissionCall~.~MySequencer \then \\
	ret := this~.~getNextMission() \circseq \\
    getNextMissionRet~.~MySequencer~!~ret  \then \\
\Skip
\circblockclose	
\end{circusaction}

