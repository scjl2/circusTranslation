\begin{circusaction}
InitializePhase \circdef \\
\circblockopen
  initializeCall~.~MissionAMID \then \\
	register~!~MT1SID~!~MissionAMID \then \\
	register~!~MT2SID~!~MissionAMID \then \\
	register~!~MT3SID~!~MissionAMID \then \\
  initializeRet~.~MissionAMID \then \\
