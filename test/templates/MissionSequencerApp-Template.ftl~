\begin{circus}
\circprocess ${MissionSequencerID}App \circdef \circbegin\\ 
\end{circus}
   
\begin{circusaction}
GetNextMission \circdef \\
\circblockopen
    getNextMissionCall~.~${MissionSequencerID} \then \\
	
	getNextMissionRet~.~${MissionSequencerID}~!~${MissionID}  \then \\
	\Skip
\circblockclose	
\end{circusaction}

\begin{circusaction}
Methods \circdef  \\
\circblockopen
	GetNextMission \\
\circblockclose 
\circseq Methods
\end{circusaction}
 
